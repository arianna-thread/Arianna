	html, body{
	margin:0;
	padding: 0;
	border: 0;
}

#map{
	background-image: url(maze.png);
	width: 1374px;
	height: 1374px;
	position: absolute;

}
#mapContainer{
	position:absolute;
}

.cursor{

	position: absolute;
	-webkit-transform:rotate(15deg);
}
.curr{
position: absolute;

}
.succ{
	position: absolute;

}
img{
-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
-webkit-focus-ring-color: rgba(0, 0, 0, 0);
}

.tooltip{
	position: absolute;
	width: 200px;
	height: 70px;
	opacity:0.9;
	border-style: solid;
	border-width: 2px;
	border-radius:15px;
	background-color: white;
	font-size: 50px ;
	font-weight: 800;
	text-align: center;


}
.rightArrow{
	position: absolute;
	height:40px;
	width: 40px;
	top: 13px;
	right: 4px;
}
