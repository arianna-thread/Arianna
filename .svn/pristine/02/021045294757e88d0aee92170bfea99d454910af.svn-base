.tooltip{
	white-space: nowrap;
    position: absolute;
    box-shadow: 5px 5px 30px black;
    border-style: solid;
    border-width: 0;
    border-radius: 7px;
    background-color: rgba(227,227,227,0.95);
    background-image: url("./rightArrow.png");
    background-repeat: no-repeat;
    background-position: right;
    background-size:40px;
    text-align: center;
    padding: 20px 22px;
    display: inline;
    width: auto;
}
.tooltip_content{
    padding-right: 30px;
    display: inline;
}

